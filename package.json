{
  "name": "@dannysir/js-te",
  "version": "0.3.0",
  "description": "JavaScript test library",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./src/mock/store.js": "./src/mock/store.js"
  },
  "bin": {
    "js-te": "./bin/cli.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c",
    "test": "node bin/cli.js",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "bin/",
    "dist/",
    "utils/",
    "src/",
    "babelTransformImport.js",
    "constants.js"
  ],
  "keywords": [
    "javascript",
    "test",
    "testing",
    "jest",
    "mocking"
  ],
  "author": "dannysir",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dannysir/js-te-package"
  },
  "bugs": {
    "url": "https://github.com/dannysir/js-te-package/issues"
  }
}
